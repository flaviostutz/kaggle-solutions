    "#simplest multiprocessing\n",
    "from multiprocessing import Pool\n",
    "import multiprocessing as mp\n",
    "\n",
    "#mp.set_start_method('spawn')\n",
    "\n",
    "n_processes = max(1,mp.cpu_count()-1)"
   ]
  },
  {
